{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "FileLogger": {
      "Options": {
        "FolderPath": "logs",
        "FilePath": "log [timestamp]_[counter].txt",
        "MaxByteSize": 30000
      },
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
